\lang american
Set 
\begin_inset Formula $d_{\max}$
\end_inset

 to denote the highest level of defensiveness attained.
\begin_inset Newline newline
\end_inset

Let 
\begin_inset Formula $B_{1/2}$
\end_inset

 denote the level of mosquito density at which defensiveness is half its maximum,
 
\begin_inset Formula $d\left(B_{1/2}\right)=d_{1/2}$
\end_inset

.
\begin_inset Formula 
\begin{align*}
d\left(B_{1/2}\right) & =\frac{1}{2}d_{\max}\\
d_{\max}\frac{a_{d}B_{1/2}^{r_{d}}}{d_{\max}+a_{d}B_{1/2}^{r_{d}}} & =\frac{1}{2}d_{\max}\\
a_{d}B_{1/2}^{r_{d}} & =d_{\max}\\
a_{d} & =d_{\max}B_{1/2}^{-r_{d}}\\
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
d\left(B\right) & =d_{\max}\left(\frac{B^{r_{d}}}{B_{1/2}^{r_{d}}+B^{r_{d}}}\right)
\end{align*}

\end_inset

Let 
\begin_inset Formula $d_{1/2}=\frac{1}{2}d_{\max}$
\end_inset

 be the value of defensiveness at which 
\begin_inset Formula $p_{X}$
\end_inset

 is half its maximum value,
 
\begin_inset Formula $p_{X}\left(d_{1/2}\right)=\frac{1}{2}\bar{p}_{X}$
\end_inset

.
 
\begin_inset Formula 
\begin{align*}
p_{X}\left(d_{1/2}\right) & =\frac{1}{2}\bar{p}_{X}\\
\bar{p}_{X}\left(1-\left(1-\frac{p_{\min}}{\bar{p}_{X}}\right)\frac{2^{-r_{X}}\left(d_{\max}^{r_{X}}\right)}{2^{-r_{X}}\left(d_{\max}^{r_{X}}\right)+a_{X}b_{X}2^{-r_{X}}\left(d_{\max}^{r_{X}}\right)}\right) & =\frac{1}{2}\bar{p}_{X}\\
\bar{p}_{X}\left(1-\left(1-\frac{p_{\min}}{\bar{p}_{X}}\right)\frac{1}{1+a_{X}b_{X}}\right) & =\frac{1}{2}\bar{p}_{X}\\
a_{X} & =\frac{2}{\bar{p}_{X}}\left(\frac{\bar{p}_{X}}{2}-p_{\min}\right)
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
p_{X}\left(d\right) & =\bar{p}_{X}\left(1-\left(1-\frac{p_{\min}}{\bar{p}_{X}}\right)\frac{\frac{1}{2}\bar{p}_{X}d^{r_{X}}}{\frac{1}{2}\bar{p}_{X}d^{r_{X}}+\left(\frac{1}{2}\bar{p}_{X}-p_{\min}\right)\left(d_{\max}-d\right)^{r_{X}}}\right)\\
p_{X}\left(d\right) & =\bar{p}_{X}\left(\frac{\frac{1}{2}p_{\min}d^{r_{X}}+\left(\frac{1}{2}\bar{p}_{X}-p_{\min}\right)\left(d_{\max}-d\right)^{r_{X}}}{\frac{1}{2}\bar{p}_{X}d^{r_{X}}+\left(\frac{1}{2}\bar{p}_{X}-p_{\min}\right)\left(d_{\max}-d\right)^{r_{X}}}\right)\\
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
p_{X}\left(0\right) & =\bar{p}_{X}\\
p_{X}\left(0\right) & =\bar{p}_{X}\\
\\\\p_{X}\left(d_{1/2}\right) & =\frac{1}{2}\bar{p}_{X}\\
p_{X}\left(\frac{1}{2}d_{\max}\right) & =\bar{p}_{X}\left(\frac{1}{2}\frac{\left(\bar{p}_{X}-\frac{1}{2}p_{\min}\right)}{\bar{p}_{X}-p_{\min}}\right)\\
\\\\p_{X}\left(d_{\max}\right) & =p_{\min}
\end{align*}

\end_inset

Note that 
\begin_inset Formula $p_{\min}$
\end_inset

 is the smallest value of 
\begin_inset Formula $p_{X}$
\end_inset

 attained.
 
\begin_inset Formula 
