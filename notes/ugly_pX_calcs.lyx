p_{X}\left(B\right) & =\bar{p}_{X}\left(\frac{\frac{1}{2}p_{\min}\left(B^{r_{d}}\right)^{r_{X}}+\left(\frac{1}{2}\bar{p}_{X}-p_{\min}\right)\left(B_{1/2}^{r_{d}}\right)^{r_{X}}}{\frac{1}{2}\bar{p}_{X}\left(B^{r_{d}}\right)^{r_{X}}+\left(\frac{1}{2}\bar{p}_{X}-p_{\min}\right)\left(B_{1/2}^{r_{d}}\right)^{r_{X}}}\right)\\
p_{X}\left(B\right) & =\bar{p}_{X}\left(\frac{\frac{1}{2}p_{\min}B^{r}+\left(\frac{1}{2}\bar{p}_{X}-p_{\min}\right)B_{1/2}^{r}}{\frac{1}{2}\bar{p}_{X}B^{r}+\left(\frac{1}{2}\bar{p}_{X}-p_{\min}\right)B_{1/2}^{r}}\right)\\
p_{X}\left(B\right) & =\bar{p}_{X}\left(1-\frac{\frac{1}{2}\left(\bar{p}_{X}-p_{\min}\right)B^{r}}{\frac{1}{2}\bar{p}_{X}B^{r}+\left(\frac{1}{2}\bar{p}_{X}-p_{\min}\right)B_{1/2}^{r}}\right)\\
