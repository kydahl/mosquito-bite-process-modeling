\end_inset


\begin_inset Formula 
\[
d\left(B\right)=\frac{\left(B/B_{1/2}\right)^{r_{d}}}{1+\left(B/B_{1/2}\right)^{r_{d}}},
\]

\end_inset


\begin_inset Formula 
\begin{align*}
p_{X}\left(B\right) & =\frac{\bar{p}_{X}}{1+\left(2\frac{\left(B/B_{1/2}\right)^{r_{d}}}{1+\left(B/B_{1/2}\right)^{r_{d}}}\right)^{r_{X}}}\\
p_{X}\left(B\right) & =\frac{\bar{p}_{X}}{1+\left(\frac{2\left(B/B_{1/2}\right)^{r_{d}}}{1+\left(B/B_{1/2}\right)^{r_{d}}}\right)^{r_{X}}}\frac{\left(1+\left(B/B_{1/2}\right)^{r_{d}}\right)^{r_{X}}}{\left(1+\left(B/B_{1/2}\right)^{r_{d}}\right)^{r_{X}}}\\
p_{X}\left(B\right) & =\bar{p}_{X}\frac{\left[1+\left(B/B_{1/2}\right)^{r_{d}}\right]^{r_{X}}}{\left[1+\left(B/B_{1/2}\right)^{r_{d}}\right]^{r_{X}}+2^{r_{X}}\left(B/B_{1/2}\right)^{r_{d}r_{X}}}\\
p_{X}\left(B\right) & =\bar{p}_{X}\frac{\left[1+B_{1/2}^{-r_{d}}B^{r_{d}}\right]^{r_{X}}}{\left[1+B_{1/2}^{-r_{d}}B^{r_{d}}\right]^{r_{X}}+2^{r_{X}}B_{1/2}^{-r_{d}r_{X}}B^{r_{d}r_{X}}}\\
p_{X}\left(B\right) & =\bar{p}_{X}\frac{\left(1+a_{d}B^{r_{d}}\right)^{r_{X}}}{\left(1+a_{d}B^{r_{d}}\right)^{r_{X}}+\left(2a_{d}B^{r_{d}}\right)^{r_{X}}}
\end{align*}

\end_inset


\begin_inset Formula 
\begin{align*}
\frac{a_{X}d_{\max}^{r_{X}}}{1+a_{X}b_{X}d_{\max}^{r_{X}}} & =1\\
a_{X}d_{\max}^{r_{X}} & =1+a_{X}b_{X}d_{\max}^{r_{X}}\\
\left(1-b_{X}\right)a_{X} & =1\\
a_{X} & =1/\left(1-b_{X}\right)
\end{align*}

